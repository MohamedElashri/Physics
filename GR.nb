}], "Text",
 CellChangeTimes->{{3.691338229708471*^9, 3.6913382352937727`*^9}, {
  3.80827695214835*^9, 
  3.8082769829187403`*^9}},ExpressionUUID->"7d415b5f-6396-44ae-81a8-\
c44d02be53ae"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], "\[Mu]\[Nu]"], "=", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{
      SuperscriptBox["g", "\[Lambda]\[Sigma]"], "(", 
      RowBox[{
       RowBox[{
        SubscriptBox["\[PartialD]", "\[Mu]"], " ", 
        SubscriptBox["g", "\[Sigma]\[Nu]"]}], "+", 
       RowBox[{
        SubscriptBox["\[PartialD]", "\[Nu]"], " ", 
        SubscriptBox["g", "\[Sigma]\[Mu]"]}], "-", 
       RowBox[{
        SubscriptBox["\[PartialD]", "\[Sigma]"], " ", 
        SubscriptBox["g", "\[Mu]\[Nu]"]}]}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"ddc31a54-4d0a-4ab9-86eb-cec999adb969"],
 ","
}], "Text",ExpressionUUID->"f64bbdfd-f97b-4d2f-9f29-2148c48d6935"],

Cell[TextData[{
 " ( ",
